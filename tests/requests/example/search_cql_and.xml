      <ns1:profile>test</ns1:profile>
      <ns1:queryLanguage>cqleng</ns1:queryLanguage>
      <ns1:queryDebug>1</ns1:queryDebug>
      <ns1:start>1</ns1:start>
      <ns1:stepValue>1</ns1:stepValue>
    </ns1:searchRequest>
  </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
